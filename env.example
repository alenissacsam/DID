
# =============================================================================
# Core deployer settings
# =============================================================================
# Deployer EOA private key used by forge script --private-key $PRIVATE_KEY
# NOTE: use a throwaway key for testnets; never commit a real key.
PRIVATE_KEY=

# RPC endpoint used by forge script --rpc-url $RPC_URL
RPC_URL=


# =============================================================================
# Core contracts (for wiring or re-use flows)
# If you re-run umbrella/full deploy scripts, these are produced for you; otherwise
# set them to existing deployments to wire roles or add modules incrementally.
# =============================================================================
# VerificationLogger contract address
VERIFICATION_LOGGER=
# Some scripts use this alias name instead
LOGGER_ADDRESS=

# TrustScore contract address (two aliases used by different scripts)
TRUST_SCORE=
TRUST_SCORE_ADDRESS=

# UserIdentityRegistry contract address
REGISTRY_ADDRESS=


# =============================================================================
# Identity interactions
# =============================================================================
# Optional: target user address for registration/metadata scripts
USER_ADDRESS=
# Optional: off-chain metadata URI to associate to a user in the registry
IDENTITY_METADATA_URI=


# =============================================================================
# Account Abstraction / Bundler
# =============================================================================
# EIP-4337 EntryPoint address for your network (e.g., Sepolia v0.6)
ENTRYPOINT_ADDRESS=
# Bundler service address allowed in factory (optional restriction)
BUNDLER_ADDRESS=
# Factory sizing parameter (optional). Default used if unset.
MAX_ACCOUNTS_PER_USER=


# =============================================================================
# Alchemy Paymaster (optional AA gas sponsorship)
# =============================================================================
# Policy ID configured in Alchemy dashboard
ALCHEMY_POLICY_ID=
# Alchemy App ID (a.k.a. application identifier)
ALCHEMY_APP_ID=
# ERC-7677 compatible paymaster address (if applicable)
ALCHEMY_PAYMASTER=


# =============================================================================
# ZK stack (verifiers and manager)
# =============================================================================
# Whether to deploy a new ZKProofManager in script/zk/DeployAndWireZK.s.sol
DEPLOY_ZKPM=true
# Address of existing ZKProofManager if reusing instead of deploying
ZKPM_ADDRESS=

# One of the scripts expects this root to be anchored via GlobalCredentialAnchor
# Provide a bytes32 hex value (e.g., 0xabc...)
ZK_ROOT=

# Verifier addresses (two naming schemes are used by different scripts).
# Use the group required by the script you run.
# Group A (DeployAndWireZK.s.sol)
AGE_VERIFIER_ADDR=
AGE_MAX_VERIFIER_ADDR=
ATTR_VERIFIER_ADDR=
INCOME_VERIFIER_ADDR=

# Group B (DeployZK.s.sol)
AGE_GTE_VERIFIER=
AGE_LTE_VERIFIER=
ATTR_EQ_VERIFIER=
INCOME_GTE_VERIFIER=


# =============================================================================
# Role wiring helpers
# =============================================================================
# Used by script/roles/GrantLoggerRole.s.sol to grant LOGGER_ROLE
GRANTEE_ADDRESS=


# =============================================================================
# STORAGE / IPFS (for tools/clients outside Solidity)
# =============================================================================
# Pinata JWT (access token) for uploading files via Pinata SDK.
# Create from your Pinata dashboard. Value typically begins with `eyJ...`.
PINATA_JWT=
